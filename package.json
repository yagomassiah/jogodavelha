{
  "name": "tic-tac-toe-vue",
  "version": "1.0.0",
  "description": "Ultimate tic-tac-toe in Vue",
  "homepage": "https://ldgit.github.io/vue-tic-tac-toe/",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "webpack-dev-server --open --mode=development",
    "lint": "eslint --ext .vue,.js ./",
    "test:all": "npm run lint && npm t",
    "deploy": "npm run test:all && npm run build:prod && rm -rf docs/* && cp -R dist/. docs/",
    "coverage": "npm t -- --coverage --coverageReporters=text-lcov | coveralls"
  },
  "author": "Danko Lučić",
  "license": "MIT",
  "dependencies": {
    "node-sass": "^4.11.0",
    "sass-loader": "^13.2.0",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.3.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "coveralls": "^3.0.2",
    "css-loader": "^6.7.3",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jsdom": "^13.1.0",
    "style-loader": "^3.3.1",
    "vue-jest": "^4.0.1",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.1.10"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
}
